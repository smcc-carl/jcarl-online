---
layout: base.njk
templateEngineOverride: njk, md
---

<section class="px-[20%]">
<div class="container py-16 leading-loose ">

  
        <img class="rounded-3xl" src="{{ image }}" alt="{{ img-alt }}">
        <p class="pt-16 pb-4 text-4xl font-semibold">{{title}}</p>
        <p class="py-4 px-6">{{intro}}</p>
        <p class='font-bold text-2xl py-8'>{{header1}}</p>
        <p class="px-6">{{desc1}}</p>
        <p class='font-bold text-2xl py-8'>{{header2}}</p>
        <p class="px-6">{{desc2}}</p>
        <p class='font-bold text-2xl py-8'>{{header3}}</p>
        <p class="px-6">{{desc3}}</p>
        <p class='font-bold text-2xl py-8'>{{header4}}</p>
        <p class="px-6">{{desc4}}</p>
        <p class='font-bold text-2xl py-8'>Conclusion</p>

        <p class='px-6'>{{conclusion}}</p>

</div>




</section>